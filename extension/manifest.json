{
    "name": "YouTube sticky player",
    "version": "1.0.3",
    "manifest_version": 2,
    "description": "__MSG_ext_desc__",
    "default_locale": "en",
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["youtube_sticky_player.js"],
            "css": ["youtube_sticky_player.css"]
        }
    ],
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    }
}
